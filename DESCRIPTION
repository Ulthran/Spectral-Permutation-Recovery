Package: SpectralPermutationRecovery
Title: Optimal Permutation Recovery in Permuted Monotone Matrix Model
Version: 0.0.0.9000
Authors@R: 
    c(
      person("Rong", "Ma", , "rongm@stanford.edu", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-9248-8503")),
